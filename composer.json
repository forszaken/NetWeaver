{
    "name": "koshagol/netweaver",
    "type": "project",
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App",
            "NetWeaver\\": "src/NetWeaver/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\App\\": "tests/App",
            "Test\\NetWeaver\\": "tests/NetWeaver"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "friendsofphp/php-cs-fixer": "^3.48"
    },
    "scripts": {
        "test": "phpunit --colors=always"
    }
}
